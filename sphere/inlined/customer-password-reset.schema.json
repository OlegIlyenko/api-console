{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Set a new password using a token.",
  "required":["id","version","tokenValue","newPassword"],
  "properties":{
    "id":{
      "type":"string",
      "description":"The unique ID of this object"
    },
    "version":{
      "type":"number",
      "description":"The current version"
    },
    "tokenValue":{
      "type":"string"
    },
    "newPassword":{
      "type":"string"
    }
  }
}