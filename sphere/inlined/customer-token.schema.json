{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Customer Token",
  "required":["id","customerId","createdAt","lastModifiedAt","expiresAt","value"],
  "properties":{
    "id":{
      "type":"string",
      "description":"The unique ID of this object"
    },
    "customerId":{
      "type":"string"
    },
    "createdAt":{
      "type":"string",
      "format":"date-time",
      "description":"Date of the creation"
    },
    "lastModifiedAt":{
      "type":"string",
      "format":"date-time",
      "description":"Date of last modification"
    },
    "expiresAt":{
      "type":"string"
    },
    "value":{
      "type":"string"
    }
  }
}