{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "title":"Set a new password using a token.",
  "required": ["id", "version", "tokenValue", "newPassword"],

  "properties": {
    "id": {
      "$ref": "id.schema.json"
    },
    "version": {
      "$ref": "version.schema.json"
    },
    "tokenValue": {
      "type": "string"
    },
    "newPassword": {
      "type": "string"
    }
  }
}
